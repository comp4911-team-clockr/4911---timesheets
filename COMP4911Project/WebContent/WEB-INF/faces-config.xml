<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">
	<navigation-rule>
		<from-view-id>/indexproto.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>loggedin</from-outcome>
			<to-view-id>/loggedin.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/EmployeesHR.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>AddEmployee</from-outcome>
			<to-view-id>/AddEmployee.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/projects.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>addProject</from-outcome>
			<to-view-id>/AddProjects.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>	
	<navigation-rule>
		<from-view-id>/WorkPackages.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>addWP</from-outcome>
			<to-view-id>/AddWorkPackage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>		
</faces-config>